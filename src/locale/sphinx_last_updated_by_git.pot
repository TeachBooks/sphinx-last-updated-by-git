#: src/sphinx_last_updated_by_git.py
#. Format string for attribution when showing only the last editor
#. Example: "by John Doe"
msgid "by %(author)s"
msgstr ""

#: src/sphinx_last_updated_by_git.py
#. Format string for attribution when showing all file editors
#. Example: "edited by Alice, Bob, and Carol"
msgid "edited by %(author)s"
msgstr ""

#: src/sphinx_last_updated_by_git.py
#. Format string for joining exactly two author names
#. Example in English: "Alice and Bob"
#. The word "and" should be translated appropriately for each language
msgid "%(author1)s and %(author2)s"
msgstr ""

#: src/sphinx_last_updated_by_git.py
#. Format string for joining three or more authors
#. Example in English: "Alice, Bob, and Carol"
#. The comma and "and" placement follows English conventions
#. Translators should adapt punctuation and conjunctions for their language
msgid "%(authors)s, and %(last_author)s"
msgstr ""
